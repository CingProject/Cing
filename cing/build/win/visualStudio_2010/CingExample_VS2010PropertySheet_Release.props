      <StringPooling>true</StringPooling>
      <RuntimeLibrary>MultiThreadedDLL</RuntimeLibrary>
      <WarningLevel>Level3</WarningLevel>
      <DebugInformationFormat>ProgramDatabase</DebugInformationFormat>
    </ClCompile>
    <Link>
      <AdditionalDependencies>OgreMain.lib;CameraControlSystem.lib;CEGUIBase.lib;CEGUIOgreRenderer.lib;OIS.lib;cv210.lib;cvaux210.lib;cxcore210.lib;highgui210.lib;ptypes.lib;BulletCollision.lib;BulletDynamics.lib;LinearMath.lib;ConvexDecomposition.lib;videoInput.lib;fmodex_vc.lib;RtMidi.lib;winmm.lib;tinyxml_STL.lib;LibXML.lib;libvlc.dll.a;libvlccore.dll.a;gstreamer-0.10.lib;gthread-2.0.lib;glib-2.0.lib;gobject-2.0.lib;gstvideo-0.10.lib;gstapp-0.10.lib;gstbase-0.10.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>..\..\cing\libraries\win\visualStudio2008\BetaCairo\lib;..\..\cing\libraries\win\visualStudio2008\Bullet\lib;..\..\cing\libraries\win\visualStudio2008\cairo\lib;..\..\cing\libraries\win\visualStudio2008\Fmod\lib;..\..\cing\libraries\win\visualStudio2008\Ogre3d\lib;..\..\cing\libraries\win\visualStudio2008\Ogre3d\boost_1_44\lib;..\..\cing\libraries\win\visualStudio2008\Ogre-CameraControlSystem\lib;..\..\cing\libraries\win\visualStudio2008\CEGUI\lib;..\..\cing\libraries\win\visualStudio2008\OgreBullet\lib;..\..\cing\libraries\win\visualStudio2008\OpenCV\lib;..\..\cing\libraries\win\visualStudio2008\PortVideo\lib;..\..\cing\libraries\win\visualStudio2008\PTypes\lib;..\..\cing\libraries\win\visualStudio2008\RtMidi\lib;..\..\cing\libraries\win\visualStudio2008\VideoInput\lib;..\..\cing\libraries\win\visualStudio2008\OgreCollada\lib;..\..\cing\libraries\win\visualStudio2008\LibVLC\lib;..\..\cing\libraries\win\visualStudio2008\TinyXML\lib;..\..\cing\libraries\win\visualStudio2008\GStreamer\lib;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <IgnoreSpecificDefaultLibraries>atlthunk;libc.lib;libcmt.lib;libcd.lib;libcmtd.lib;msvcrtd.lib;%(IgnoreSpecificDefaultLibraries)</IgnoreSpecificDefaultLibraries>
      <GenerateDebugInformation>true</GenerateDebugInformation>
      <SubSystem>NotSet</SubSystem>
      <OptimizeReferences>false</OptimizeReferences>
      <EnableCOMDATFolding>true</EnableCOMDATFolding>
    </Link>
    <PostBuildEvent>
      <Command>xcopy "$(SolutionDir)cing\bin\win\visualStudio2008\$(Configuration)" "$(OutDir)" /i /d /y /s

REM Check if it was ok
if errorlevel 1 goto BuildEventFailed
goto BuildEventOK

:BuildEventFailed
echo POSTBUILDSTEP for $(ProjectName) FAILED
exit 1

:BuildEventOK
echo POSTBUILDSTEP for $(ProjectName) COMPLETED OK
</Command>
    </PostBuildEvent>
  </ItemDefinitionGroup>
</Project>