<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <CING_ROOT>..\..\cing\</CING_ROOT>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>11.0.60610.1</_ProjectFileVersion>
    <OutDir>$(ProjectDir)$(Configuration)\</OutDir>
    <IntDir>obj\$(Configuration)\</IntDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <Optimization>MaxSpeed</Optimization>
      <IntrinsicFunctions>true</IntrinsicFunctions>
      <FavorSizeOrSpeed>Speed</FavorSizeOrSpeed>
      <OmitFramePointers>true</OmitFramePointers>
      <WholeProgramOptimization>true</WholeProgramOptimization>
      <AdditionalIncludeDirectories>$(CING_ROOT);$(CING_ROOT)src\;$(CING_ROOT)libraries\win\visualStudio2008\OpenCV\include;$(CING_ROOT)libraries\win\visualStudio2008\LibVLC\include;$(CING_ROOT)libraries\win\visualStudio2008;$(CING_ROOT)libraries\win\visualStudio2008\GStreamer\include\gstreamer-0.10;$(CING_ROOT)libraries\win\visualStudio2008\GStreamer\include\glib-2.0;$(CING_ROOT)libraries\win\visualStudio2008\GStreamer\include\libxml2;$(CING_ROOT)libraries\win\visualStudio2008\GStreamer\include;$(CING_ROOT)libraries\win\visualStudio2008\Ogre3d\include\OGRE;$(CING_ROOT)libraries\win\visualStudio2008\Ogre3d\include\OIS;$(CING_ROOT)libraries\win\visualStudio2008\Ogre3d\boost;$(CING_ROOT)libraries\win\visualStudio2008\Ogre-CameraControlSystem\include;$(CING_ROOT)libraries\win\visualStudio2008\CEGUI\include;$(CING_ROOT)libraries\win\visualStudio2008\Bullet\src;$(CING_ROOT)libraries\win\visualStudio2008\OgreBullet\Collisions\include;$(CING_ROOT)libraries\win\visualStudio2008\OpenAL\include;$(CING_ROOT)..\cing-ings\;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>WIN32;NDEBUG;_WINDOWS;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <StringPooling>true</StringPooling>
      <RuntimeLibrary>MultiThreadedDLL</RuntimeLibrary>
      <WarningLevel>Level3</WarningLevel>
      <DebugInformationFormat>ProgramDatabase</DebugInformationFormat>
    </ClCompile>
    <Link>
      <AdditionalDependencies>OgreMain.lib;OgreOverlay.lib;OIS.lib;cv210.lib;cvaux210.lib;cxcore210.lib;highgui210.lib;ptypes.lib;fmodex_vc.lib;winmm.lib;tinyxml_STL.lib;gstreamer-0.10.lib;gthread-2.0.lib;glib-2.0.lib;gobject-2.0.lib;gstvideo-0.10.lib;gstapp-0.10.lib;gstbase-0.10.lib;videoInput.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>$(CING_ROOT)libraries\win\visualStudio2008\BetaCairo\lib;$(CING_ROOT)libraries\win\visualStudio2008\Bullet\lib;$(CING_ROOT)libraries\win\visualStudio2008\cairo\lib;$(CING_ROOT)libraries\win\visualStudio2008\Fmod\lib;$(CING_ROOT)libraries\win\visualStudio2008\Ogre3d\lib;$(CING_ROOT)libraries\win\visualStudio2008\Ogre3d\boost\lib;$(CING_ROOT)libraries\win\visualStudio2008\Ogre-CameraControlSystem\lib;$(CING_ROOT)libraries\win\visualStudio2008\CEGUI\lib;$(CING_ROOT)libraries\win\visualStudio2008\OgreBullet\lib;$(CING_ROOT)libraries\win\visualStudio2008\OpenCV\lib;$(CING_ROOT)libraries\win\visualStudio2008\PortVideo\lib;$(CING_ROOT)libraries\win\visualStudio2008\PTypes\lib;$(CING_ROOT)libraries\win\visualStudio2008\RtMidi\lib;$(CING_ROOT)libraries\win\visualStudio2008\VideoInput\lib;$(CING_ROOT)libraries\win\visualStudio2008\OgreCollada\lib;$(CING_ROOT)libraries\win\visualStudio2008\LibVLC\lib;$(CING_ROOT)libraries\win\visualStudio2008\TinyXML\lib;$(CING_ROOT)libraries\win\visualStudio2008\GStreamer\lib;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <IgnoreSpecificDefaultLibraries>atlthunk;libc.lib;libcmt.lib;libcd.lib;libcmtd.lib;msvcrtd.lib;%(IgnoreSpecificDefaultLibraries)</IgnoreSpecificDefaultLibraries>
      <GenerateDebugInformation>true</GenerateDebugInformation>
      <SubSystem>NotSet</SubSystem>
      <OptimizeReferences>false</OptimizeReferences>
      <EnableCOMDATFolding>true</EnableCOMDATFolding>
    </Link>
    <PostBuildEvent>
      <Command>xcopy "$(CING_ROOT)bin\win\visualStudio2008\$(Configuration)" "$(OutDir)" /i /d /y /s

REM Check if it was ok
if errorlevel 1 goto BuildEventFailed
goto BuildEventOK

:BuildEventFailed
echo POSTBUILDSTEP for $(ProjectName) FAILED
exit 1

:BuildEventOK
echo POSTBUILDSTEP for $(ProjectName) COMPLETED OK

</Command>
    </PostBuildEvent>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="CING_ROOT">
      <Value>$(CING_ROOT)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>